%{
#include <string.h>
#include "parser.h"

typedef struct KeywordTableEntry KeywordTableEntry;
%}
%language=ANSI-C
%define lookup-function-name __Yog_lookup_keyword__
%struct-type
struct KeywordTableEntry { const char* name; enum yytokentype type; };
%readonly-tables
%%
__LINE__, t__LINE__
as, tAS
break, tBREAK
class, tCLASS
def, tDEF
do, tDO
elif, tELIF
else, tELSE
end, tEND
except, tEXCEPT
false, tFALSE
finally, tFINALLY
if, tIF
next, tNEXT
nonlocal, tNONLOCAL
return, tRETURN
true, tTRUE
try, tTRY
while, tWHILE
%%
