%{
#include <string.h>
#include "parser.h"

typedef struct KeywordTableEntry KeywordTableEntry;
%}
%language=ANSI-C
%define lookup-function-name __Yog_lookup_keyword__
%struct-type
struct KeywordTableEntry { const char* name; unsigned int type; };
%readonly-tables
%%
__FILE__, TK_FILE
__LINE__, TK_LINE
as, TK_AS
break, TK_BREAK
class, TK_CLASS
def, TK_DEF
do, TK_DO
elif, TK_ELIF
else, TK_ELSE
end, TK_END
except, TK_EXCEPT
false, TK_FALSE
finally, TK_FINALLY
from, TK_FROM
if, TK_IF
import, TK_IMPORT
module, TK_MODULE
next, TK_NEXT
nil, TK_NIL
nonlocal, TK_NONLOCAL
raise, TK_RAISE
return, TK_RETURN
super, TK_SUPER
true, TK_TRUE
try, TK_TRY
while, TK_WHILE
%%
